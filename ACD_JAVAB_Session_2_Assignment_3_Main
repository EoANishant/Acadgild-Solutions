public class ACD_JAVAB_Session_2_Assignment_3_Main 
{
public static void main(String[] args) 
{
	int k=0;
	for(int i=1;i<=5;)
	{
		for(int j=1;j<=i;j++)
		{
			System.out.print(j);
		}
		System.out.println();
		if((i+k)>=5)
		{
			i--;
			k++;
		}
		else if(i<5)
		{
			i++;
		}
		if(k==5)
		{
			System.out.println("i m done :)");
			break;
		}
	}
}
}
